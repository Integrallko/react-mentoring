{
	"compilerOptions": {
		"target": "esnext",
		"outDir": "./dist/",
		"strictNullChecks": true,
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": "./src",
		"module": "es6",
		"removeComments": true,
		"alwaysStrict": true,
		"allowUnreachableCode": false,
		"noImplicitAny": true
	},
	"exclude": [
		"node_modules"
	]
}